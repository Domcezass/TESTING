local args = {
    [1] = game:GetService("ReplicatedStorage"):WaitForChild("Eggs"):WaitForChild("EH Egg"):WaitForChild("Pets"):WaitForChild("Easter Death"),  -- Targeting Easter Death inside EH Egg > Pets
    [2] = 1  -- Number of eggs to hatch
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("HatchEggs"):InvokeServer(unpack(args))
